{
    "resourceType": "Encounter",
    "id": "<encounter-id>",
    "status": "<encounter-status>", <!-- e.g., planned, in-progress, completed -->
    "class": {
      "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
      "code": "EMER", <!-- Emergency department encounter class -->
      "display": "Emergency"
    },
    "type": [
      {
        "coding": [
          {
            "system": "http://snomed.info/sct",
            "code": "<encounter-type-code>", <!-- e.g., SNOMED code for emergency encounter -->
            "display": "<encounter-type-display>"
          }
        ],
        "text": "<encounter-type-text>"
      }
    ],
    "subject": {
      "reference": "Patient/<patient-id>"
    },
    "participant": [
      {
        "type": [
          {
            "coding": [
              {
                "system": "http://terminology.hl7.org/CodeSystem/v3-ParticipationType",
                "code": "PPRF",
                "display": "Primary Performer"
              }
            ]
          }
        ],
        "individual": {
          "reference": "Practitioner/<practitioner-id>",
          "display": "<practitioner-display>"
        }
      },
      {
        "type": [
          {
            "coding": [
              {
                "system": "http://terminology.hl7.org/CodeSystem/v3-ParticipationType",
                "code": "ADM",
                "display": "Admitter"
              }
            ]
          }
        ],
        "individual": {
          "reference": "Practitioner/<admitter-id>",
          "display": "<admitter-display>"
        }
      }
    ],
    "period": {
      "start": "<encounter-start-date>", <!-- e.g., YYYY-MM-DDTHH:MM:SS -->
      "end": "<encounter-end-date>" <!-- optional, YYYY-MM-DDTHH:MM:SS -->
    },
    "location": [
      {
        "location": {
          "reference": "Location/<location-id>",
          "display": "<location-display>"
        },
        "status": "<location-status>" <!-- e.g., active, completed -->
      }
    ],
    "serviceProvider": {
      "reference": "Organization/<organization-id>",
      "display": "<organization-display>"
    },
    "reasonCode": [
      {
        "coding": [
          {
            "system": "http://snomed.info/sct",
            "code": "<reason-code>", <!-- e.g., SNOMED code for reason of visit -->
            "display": "<reason-display>"
          }
        ],
        "text": "<reason-text>"
      }
    ],
    "diagnosis": [
      {
        "condition": {
          "reference": "Condition/<condition-id>",
          "display": "<condition-display>"
        },
        "rank": <diagnosis-rank> <!-- e.g., 1 for primary diagnosis -->
      }
    ]
  }
  